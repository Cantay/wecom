<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">Enterprise WeChat Get Contacts Access Aoken Button></field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="wxwrok_organization.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='contacts_access_token_text']" position="after" >
                    <div class="text-muted col-md-12">
                        <button name="get_contacts_access_token" type="object" string="Click test to get token" class="oe_highlight"/>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>