<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record forcecreate="True" id="ir_cron_pull_wxwork_application_ticket" model="ir.cron">
            <field name="name">Enterprise WeChat: Pull application ticket regularly</field>
            <field name="model_id" ref="model_res_config_settings"/>
            <field name="state">code</field>
            <field name="code">model.cron_pull_application_ticket()</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">2</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
