<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="assets_frontend" name="Assets Frontend" inherit_id="website.assets_frontend">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/website_china_extends/static/src/scss/social.scss"/>
                <link rel="stylesheet" type="text/scss" href="/website_china_extends/static/src/scss/social_sidebar.scss"/>
